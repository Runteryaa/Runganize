{
  "expo": {
    "owner": "runterya",
    "name": "Rungorize",
    "slug": "rungorize",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "runify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.runterya.rungorize"
    },
    "android": {
      "permissions": [
        "READ_MEDIA_AUDIO",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#1D1B20",
        "foregroundImage": "./assets/images/icon.png",
        "backgroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.runterya.rungorize",
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-media-library",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-share-intent",
        {
          "androidIntentFilters": ["text/*", "text/uri-list"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4d31c924-fc50-45b7-aca9-ba9cb0bf7e7c"
      }
    }
  }
}
